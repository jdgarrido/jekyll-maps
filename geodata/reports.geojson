---
---
{
	"type": "FeatureCollection",
	"features": [
		{% for post in site.reportes %}
		{
			"type": "Feature",
			"geometry": {
				"type": "Point",
				"coordinates": [ {{ post.points }} ]
			},
			"properties": {
				"name": "{{ post.title }}",
				"marker-color": "#9c89cc"
			}
		}{% if forloop.rindex > 1 %},{% endif%}
	{% endfor %}
	]
}
